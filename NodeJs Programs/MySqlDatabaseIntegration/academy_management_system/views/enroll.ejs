<form method="POST" action="/enroll">

<select name="studentId">
  <% students.forEach(s => { %>
    <option value="<%= s.id %>">
      <%= s.name %>
    </option>
  <% }) %>
</select>

<select name="courseId">
  <% courses.forEach(c => { %>
    <option value="<%= c.id %>">
      <%= c.title %>
    </option>
  <% }) %>
</select>

<button type="submit">Enroll</button>

</form>
